{
    "name": "sven/artisan-view",
    "description": "Manage your views in Laravel projects through artisan",
    "keywords": ["artisan", "laravel", "views", "templates"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.*" 
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "graham-campbell/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Sven\\ArtisanView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\ArtisanView\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\ArtisanView\\ArtisanViewServiceProvider"
            ]
        }
    }
}
